.newsDetail text{
	display: block;
}
.newsDetail image{
	width: 100%;
	height: 500rpx;
}
.newsDetail .newsTit{
	font-size: 18px;
	line-height: 50px;
	padding: 0 10px;
}
.newsDetail .dateColle{
	display: flex;
	height: 80rpx;
}
.newsDetail .newsDate{
	color: #999999;
	line-height: 24px;
	padding: 0 10px;
	flex-grow:1;
}
.newsDetail .dateColle .collection{
	width: 60rpx;
	height: 60rpx;
	margin-right: 10rpx;
	background: url('/images/icons/collection.png') center center no-repeat #cccccc;
	background-size: 71% auto;
	border-radius: 50%;
}
.newsDetail .dateColle .collection.cur{
	background-color: #f6b644;
}
.newsDetail .dateColle .share{
	width: 60rpx;
	height: 60rpx;
	margin-right: 10rpx;
	background: url('/images/icons/share.png') center center no-repeat #42c432;
	background-size: 80% auto;
	border-radius: 50%;
}
.newsDetail .newsDis{
	line-height: 25px;
	padding-top: 10px;
	padding: 10px;
}